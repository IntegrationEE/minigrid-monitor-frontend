{
  "name": "minigrid",
  "version": "4.0.0",
  "scripts": {
    "serve": "ng serve --port 8080",
    "build": "node ./node_modules/@angular/cli/bin/ng build",
    "build:develop": "node ./node_modules/@angular/cli/bin/ng build --configuration=dev",
    "build:release": "node ./node_modules/@angular/cli/bin/ng build --configuration=release",
    "build:demo": "node ./node_modules/@angular/cli/bin/ng build --configuration=demo",
    "build:master": "node ./node_modules/@angular/cli/bin/ng build --configuration=prod",
    "build:pakistan": "node ./node_modules/@angular/cli/bin/ng build --configuration=pakistan",
    "test": "node ./node_modules/@angular/cli/bin/ng test --browsers Chrome --watch",
    "test:ci": "node ./node_modules/@angular/cli/bin/ng test --browsers ChromeHeadless --no-progress --code-coverage --source-map=false",
    "lint": "node ./node_modules/@angular/cli/bin/ng lint",
    "e2e": "node ./node_modules/@angular/cli/bin/ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint",
      "pre-build": "npm install -q && npm run lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/file-input": "^4.0.1",
    "@angular/animations": "^11.0.2",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/forms": "^11.0.2",
    "@angular/material": "^11.0.0",
    "@angular/platform-browser": "^11.0.2",
    "@angular/platform-browser-dynamic": "^11.0.2",
    "@angular/router": "^11.0.2",
    "@asymmetrik/ngx-leaflet": "^8.1.0",
    "@auth0/angular-jwt": "^5.0.2",
    "@emmaramirez/dom-to-image": "^3.0.2",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@ngneat/until-destroy": "^8.0.3",
    "@rinminase/ng-charts": "^3.2.1",
    "chartjs-plugin-annotation": "^0.5.7",
    "core-js": "^3.7.0",
    "downloadjs": "^1.4.7",
    "husky": "^4.3.0",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "ng5-slider": "^1.2.6",
    "ngx-cookie-service": "^10.1.1",
    "ngx-mat-select-search": "^3.1.4",
    "ngx-progressbar": "^6.0.3",
    "rxjs": "^6.6.3",
    "sha256": "^0.2.0",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular/cli": "^11.0.2",
    "@angular/compiler-cli": "^11.0.2",
    "@angular/language-service": "^11.0.2",
    "@types/downloadjs": "^1.4.2",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/leaflet": "^1.5.19",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.9",
    "@types/sha256": "^0.2.0",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ngx-spec": "^2.1.4",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "~4.0.5"
  }
